{
  "name": "aqdm",
  "version": "1.0.1",
  "description": "Simple terminal progress indicator made for any loop",
  "keywords": [
    "tqdm",
    "progress",
    "indicator",
    "for",
    "iterate",
    "iterator",
    "loop"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --minify",
    "clean": "npx rimraf node_modules && npx rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "author": {
    "name": "Antoni Silvestroviƒç",
    "email": "antoni.silvestrovic@gmail.com",
    "url": "https://antoni.ai/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bring-shrubbery/aqdm.git"
  },
  "devDependencies": {
    "@swc/core": "^1.2.194",
    "@swc/jest": "^0.2.21",
    "@types/node": "^16",
    "rimraf": "^3.0.2",
    "tsup": "^6.0.1",
    "typescript": "^4.7.2"
  }
}
